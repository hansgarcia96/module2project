<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel="stylesheet" href="/stylesheets/xtyle.css"/>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
  <script src="https://unpkg.com/axios/dist/axios.min.js"></script>

  <title>Rockets</title>
</head>
<body>
  <section class="container">
    <h1>Space X Rockets</h1>
    <div class="rockets-container">
      <div class="rocket-info">
        <div class="name"></div>
        <div class="occupation"></div>
        <div class="cartoon"></div>
        <div class="image"> <img src=""> </div>
      </div>
    </div>
    <section class="operations">
      <div class="operation">
        <button id="fetch-all">Fetch all</button>
      </div>
      <div class="operation">
        <label for="rocket-id">Search by ID: </label>
        <input type="text" name="rocket-id">
        <button id="fetch-one">Fetch one</button>
      </div>


    </section>
  </section>

  <script type="text/javascript" src="/javascripts/spacexAPIHandler.js"></script>
  <script type="text/javascript" src="/javascripts/rocketIndex.js"></script>
</body>
</html>
